{
  "name": "@cap-js/print",
  "version": "1.0.0",
  "description": "CAP plugin for SAP Cloud Print Service.",
  "repository": "@cap-js/print",
  "homepage": "https://cap.cloud.sap/",
  "main": "cds-plugin.js",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "files": [
    "index.cds",
    "lib",
    "srv"
  ],
  "keywords": [
    "sap",
    "cap",
    "cps",
    "print",
    "plugin"
  ],
  "dependencies": {
    "@cap-js/print": "file:",
    "@sap-cloud-sdk/connectivity": "^3.26.4",
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap/xssec": "^3.6.2"
  },
  "peerDependencies": {
    "@sap/cds": "^8"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.2.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds": "^8",
    "eslint": "^9.4.0",
    "eslint-plugin-jest": "^28.5.0",
    "jest": "^29.7.0"
  },
  "cds": {
    "requires": {
      "destinations": true,
      "print": {
        "[development]": {
          "kind": "print-to-console"
        },
        "[production]": {
          "destination": "SAP_Print_Service",
          "kind": "print-to-service"
        },
        "[hybrid]": {
          "destination": "SAP_Print_Service",
          "kind": "print-to-service"
        },
        "vcap": {
          "tag": "print"
        },
        "outbox": true
      },
      "kinds": {
        "print-to-console": {
          "impl": "@cap-js/print/srv/printToConsole"
        },
        "print-to-service": {
          "impl": "@cap-js/print/srv/printToPrintService"
        }
      }
    }
  },
  "scripts": {
    "test": "jest --coverage --config jest.config.js",
    "integration-test": "jest --config jest-integration.config.js",
    "lint": "npx eslint --fix . --no-cache",
    "in-cap-dev": "cd node_modules && rm -fr @sap/cds* @cap-js/db-service @cap-js/sqlite",
    "prettier": "npx -y prettier@3 --write .",
    "prettier:check": "npx -y prettier@3 --check ."
  }
}
