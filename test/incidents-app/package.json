{
  "name": "@capire/incidents",
  "version": "1.0.0",
  "dependencies": {
    "@cap-js/print": "file:../../."
  },
  "scripts": {
    "watch": "cds watch",
    "watch:hybrid": "cds watch --profile hybrid"
  },
